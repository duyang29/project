<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--BottomNavigationBar样式-->
    <declare-styleable name="BottomNavigationBar">
        <attr name="bnbElevation" format="dimension" />
        <attr name="bnbActiveColor" format="color" />
        <attr name="bnbInactiveColor" format="color" />
        <attr name="bnbBackgroundColor" format="color" />
        <attr name="bnbAnimationDuration" format="integer" />
        <attr name="bnbMode" format="enum">
            <enum name="mode_default" value="0" />
            <enum name="mode_fixed" value="1" />
            <enum name="mode_shifting" value="2" />
        </attr>
        <attr name="bnbBackgroundStyle" format="enum">
            <enum name="background_style_default" value="0" />
            <enum name="background_style_static" value="1" />
            <enum name="background_style_ripple" value="2" />
        </attr>
        <attr name="bnbAutoHideEnabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="MainBottomNavigationBar">
        <attr name="tab_active" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="FormWriteTopTitleView">
        <attr name="wvItemName" format="reference|string" />
        <attr name="wvHint" format="reference|string" />
        <attr name="wvArrowDrop" format="reference" />
        <attr name="wvEnabled" format="boolean" />
        <attr name="wvInputType">
            <enum name="text" value="0" />
            <enum name="numberDecimal" value="1" /><!--小数点-->
            <enum name="number" value="2" /><!--数字-->
        </attr>
    </declare-styleable>

    <declare-styleable name="FormSelectTopTitleView">
        <attr name="svItemName" format="reference|string" />
        <attr name="svHint" format="reference|string" />
        <attr name="svArrowDrop" format="reference" />
        <attr name="svEnabled" format="boolean" />
    </declare-styleable>

</resources>